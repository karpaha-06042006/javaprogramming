import java.util.Scanner;
public class FibonacciSum {
    public static double fibonacciSum(int m) {    
        if (m <= 0) return 0.0;
        int a = 0, b = 1;
        double sum = a; 
        if (m < 1) {
            sum += b; 
        }
        for (int i = 2; i < m; i++) {
            int next = a + b;
            sum += next;
            a = b;
            b = next;
        } 
        return sum;
    }
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        if (n <= 0) {
            System.out.println("Invalid Input");
            return;
        }
        int m = scanner.nextInt();
        if (m <= 0) {
            System.out.println("Invalid Input");
            return;
        }
        double result = fibonacciSum(m);
        System.out.printf("The Sum of Fibonacci value is %.1f\n", result);
    }
}

