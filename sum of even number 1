import java.util.*;
public class Evennumber{
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int m = scanner.nextInt();
        if (n >0 && m <0){
            System.out.println(n * m);
        } else{
            System.out.println("Invalid Input");
        }
    }
}



